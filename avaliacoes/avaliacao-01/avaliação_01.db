sqlite3 avaliacao_01

CREATE TABLE TB_CLIENTE (
	id INTEGER PRIMARY KEY AUTOINCREMENT,
	nome TEXT NOT NULL
);
CREATE TABLE TB_PRODUTO (
	id INTEGER PRIMARY KEY AUTOINCREMENT,
	QUANTIDADE_ESTOQUE FLOAT NOT NULL,
	PRECO_UNITARIO FLOAT NOT NULL
);
	
CREATE TABLE TB_NOTA_FISCAL (
	id INTEGER PRIMARY KEY AUTOINCREMENT,
	CLIENTE_ID INTEGER,
	DATA_NOTA_FISCAL DATE NOT NULL,
	FOREIGN KEY (CLIENTE_ID) REFERENCES TB_CLIENTE(id)
); 
CREATE TABLE TB_ITEM_NOTA_FISCAL (
	id INTEGER PRIMARY KEY AUTOINCREMENT,
	PRODUTO_ID INTEGER,
	QUANTIDADE_ITEM FLOAT NOT NULL,
	PRECO_ITEM FLOAT NOT NULL,
	TB_NOTA_FISCAL_ID INTEGER,
	FOREIGN KEY (PRODUTO_ID) REFERENCES TB_PRODUTO(id)
	FOREIGN KEY (TB_NOTA_FISCAL_ID) REFERENCES TB_NOTA_FISCAL(id)
);
